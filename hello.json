       
        {
          "id": "world",
          "cmd": "helloworld -addr=:$PORT0 -protocol=grpc -text=Hello -target=localhost:4142 -protocol=grpc",
          "env": {
            "TARGET_WORLD": "world"
          },
          "cpus": 0.5,
          "mem": 64.0,
          "instances": 1,
          "container": {
            "type": "DOCKER",
            "docker": {
              "image": "buoyantio/helloworld:0.1.4",
              "network": "BRIDGE",
              "portMappings": [
                { "containerPort": 0, "hostPort": 0, "protocol": "tcp" }
              ]
            }
          }        
        }
